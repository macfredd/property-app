{
  "name": "property-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "json-server": "json-server --watch db.json --port 3000",
    "dev": "concurrently \"npm run json-server\" \"npm run start\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rm -rf .angular && rm -rf node_modules/.cache && npm run build",
    "clean:win": "if exist .angular rmdir /s /q .angular && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && npm run build"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@primeuix/themes": "^1.2.1",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.4",
    "@angular/cli": "^20.0.4",
    "@angular/compiler-cli": "^20.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.2"
  }
}
